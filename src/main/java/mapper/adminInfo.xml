<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.adminInfo">

	<!-- 관리자 로그인 -->
	<select id="adminLoginConfirm" resultType="adminInfo" parameterType="Map">
		select admin_info_id, aid, aname from admin_info where aid=#{id} and apassword=#{password}
	</select>



</mapper>



